{
  "name": "coordinatepointstool",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx cucumber-js --profile runner && npm run report",
    "test:smoke": "npx cucumber-js --profile smoke",
    "test:critical": "npx cucumber-js --profile critical",
    "test:regression": "npx cucumber-js --profile regression",
    "test:validation": "npx cucumber-js --profile validation",
    "test:calculation": "npx cucumber-js --profile calculation",
    "test:edgecase": "npx cucumber-js --profile edgecase",
    "test:ui": "npx cucumber-js --profile ui",
    "test:bugs": "npx cucumber-js --profile bugs",
    "test:nobug": "npx cucumber-js --profile nobug",
    "test:predeploy": "npx cucumber-js --profile predeploy",
    "test:fullregression": "npx cucumber-js --profile fullregression",
    "test:tags": "npx cucumber-js --tags",
    "failed:test": "npx cucumber-js --profile rerun",
    "report": "npx ts-node ./src/support/reporter/HTMLReporter.ts && npx ts-node ./src/support/reporter/CucumberReporter.ts",
    "qa:test": "cross-env TEST_ENV=qa npx cucumber-js --profile runner && cross-env TEST_ENV=qa npm run report",
    "dry:test": "npx cucumber-js --profile runner --dry-run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^10.8.0",
    "fs-extra": "^11.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.9",
    "cucumber-html-reporter": "^7.1.1",
    "multiple-cucumber-html-reporter": "^3.6.2",
    "playwright": "1.56"
  }
}
